{
  "name": "v0-prompt-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:integration:watch": "jest --selectProjects integration --watch",
    "test:ai-workflow": "jest --testPathPatterns=ai-workflow",
    "analyze": "cross-env ANALYZE=true next build",
    "bundle-analyzer": "npx @next/bundle-analyzer",
    "performance:monitor": "node scripts/performance-monitor.js",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@alchemystai/sdk": "^0.3.0",
    "@heroicons/react": "^2.2.0",
    "critters": "^0.0.23",
    "next": "14.2.33",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.33",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.33",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "next-sitemap": "^4.2.3",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
